{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./frontend/tsconfig.json", "./backend/tsconfig.json"]
  },
  "plugins": ["vue", "prettier", "import"],
  "rules": {
    "arrow-parens": "off"
  }
}
