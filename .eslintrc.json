{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "airbnb/recommended",
    "prettier",
    "plugin:vue/vue3-recommended"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "warn",
    "no-debugger": "error"
  }
}
